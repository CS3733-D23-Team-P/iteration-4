<?xml version="1.0" encoding="UTF-8"?>

<?import edu.wpi.punchy_pegasi.frontend.components.PFXAccount?>
<?import edu.wpi.punchy_pegasi.frontend.icons.PFXIcon?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/"
         styleClass="header">
    <HBox styleClass="header-left-container">
        <HBox HBox.hgrow="ALWAYS" styleClass="header-search" fx:id="headerSearch">
            <PFXIcon icon="SEARCH"/>
            <Label text="Search"/>
        </HBox>
    </HBox>
    <HBox HBox.hgrow="ALWAYS"/>
    <HBox styleClass="header-right-container" HBox.hgrow="NEVER">
        <PFXAccount fx:id="pfxAccount"/>
    </HBox>
</fx:root>
